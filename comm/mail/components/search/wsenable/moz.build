Program('WSEnable')

SOURCES += [
    'WSEnable.cpp',
]

OS_LIBS += [
    'ole32',
    'shell32',
]

RCINCLUDE = 'WSEnable.rc'

# This isn't XPCOM code, but it wants to use STL so disable STL wrappers
DisableStlWrapping()

if CONFIG['CC_TYPE'] == 'clang-cl':
    AllowCompilerWarnings()  # workaround for bug 1090497 / bug 1481326

